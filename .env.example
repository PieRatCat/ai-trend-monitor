# Environment Variables (Example)
# Copy this file to .env and fill in your actual values
# .env is git-ignored for security

# Guardian API
GUARDIAN_API_KEY=your_guardian_api_key_here

# Azure Storage
AZURE_STORAGE_CONNECTION_STRING=DefaultEndpointsProtocol=https;AccountName=your_account;AccountKey=your_key;EndpointSuffix=core.windows.net

# Azure AI Language
LANGUAGE_KEY=your_language_service_key_here
LANGUAGE_ENDPOINT=https://your-language-service.cognitiveservices.azure.com/

# Azure AI Search (also needed for Streamlit app)
SEARCH_ENDPOINT=https://your-search-service.search.windows.net/
SEARCH_KEY=your_search_admin_key_here

# GitHub Models API (also needed for Streamlit app)
GITHUB_TOKEN=ghp_your_github_personal_access_token_here
